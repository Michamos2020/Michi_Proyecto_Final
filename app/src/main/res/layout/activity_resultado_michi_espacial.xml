<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/huellas"
    tools:context=".Resultado_michiEspacial">

    <TextView
        android:id="@+id/txt_resultado"
        android:layout_width="303dp"
        android:layout_height="279dp"
        android:layout_marginTop="144dp"
        android:backgroundTint="#0F1010"
        android:hint="Resultado"
        android:textColor="#EDE8E8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_regresaInicio"
        android:layout_width="154dp"
        android:layout_height="104dp"
        android:backgroundTint="#B32222"
        android:onClick="Regresar"
        android:text="@string/btn_calcular_regresa_espacial"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_resultado" />

</androidx.constraintlayout.widget.ConstraintLayout>