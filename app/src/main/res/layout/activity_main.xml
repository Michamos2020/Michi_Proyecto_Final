<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/fondo_azul2"
    android:onClick="Ejecutar"
    tools:context=".MainActivity">

    <Spinner
        android:id="@+id/spinner1"
        android:layout_width="217dp"
        android:layout_height="97dp"
        android:layout_marginTop="160dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.427"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_ejecutarprincipal"
        android:layout_width="198dp"
        android:layout_height="114dp"
        android:layout_marginTop="125dp"
        android:layout_marginBottom="296dp"
        android:background="@color/color_btn_press"
        android:backgroundTint="@color/color_btn"
        android:onClick="Ejecutar"
        android:text="@string/btn_ejecutarprincipal"
        android:textColor="#E9F3F1"
        android:textColorHighlight="#3C1D1D"
        android:textColorLink="#D8C2C2"
        app:cornerRadius="20dp"
        app:iconTint="#271F1F"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.396"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spinner"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="HardcodedText,UnknownId" />

</androidx.constraintlayout.widget.ConstraintLayout>